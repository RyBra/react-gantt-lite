{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./dist",
    "declaration": true,
    "emitDeclarationOnly": false,
    "declarationMap": true,
    "incremental": false,
    "composite": false,
    "noEmit": false,
    "skipLibCheck": true,
    "strict": true,
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ES2020",
    "baseUrl": ".",
    "paths": {}
  },
  "include": ["src"]
}

